@create Master Planet=10
&amp;SET_GREETING Master Planet=$set greeting*=*:@swi u(admin_privs, %#)=yes,{@swi words(%1)=0,{&amp;greeting me=;@pemit %#=Greeting cleared for [v(place_type)]%b[edit(secure(v(place_name)), #, ')].},{&amp;greeting me=%1;@pemit %#=Greeting set for [v(place_type)]%b[edit(secure(v(place_name)), #, ')].}},{@pemit %#=You are not authorized to use this command.}
&amp;CLEAR_GREETING Master Planet=$clear greeting*:@swi u(admin_privs, %#)=yes,{&amp;greeting me=;@pemit %#=Greeting cleared for [v(place_type)]%b[edit(secure(v(place_name)), #, ')].},{@pemit %#=You are not authorized to use this command.}
&amp;EDIT_GREETING Master Planet=$edit greeting*=*/*:@swi u(admin_privs, %#)=yes,{@swi %1=$,{@swi words(v(greeting))=0,{&amp;greeting me=%2},{@edit me/greeting=%1, {%2}}},^,{@swi words(v(greeting))=0,{&amp;greeting me=%2},{@edit me/greeting=%1, {%2}}},{@edit me/greeting={%1},{%2}};@trigger me/cleanup_edit_greeting=%#},{@pemit %#=You are not authorized to use this command.}
&amp;CLEANUP_EDIT_GREETING Master Planet=@swi words(v(greeting))=0,{@pemit %0=There is no greeting currently registered for%b[edit(secure(v(place_name)), #, ')].},{@pemit %0=The greeting for [v(place_type)]%b[edit(secure(v(place_name)), #, ')] now reads:%b%b[s(v(greeting))]}
&amp;DISPLAY_GREETING Master Planet=$display greet*:@swi u(admin_privs, %#)=yes,{@swi words(v(greeting))=0,{@pemit %#=There is no greeting currently registered for%b[edit(secure(v(place_name)), #, ')].},{@pemit %#=The greeting for [v(place_type)]%b[edit(secure(v(place_name)), #, ')] reads:%b%b[s(v(greeting))]}},{@pemit %#=You are not authorized to use this command.}
&amp;PLCU_SHANKILL Master Planet=#94
&amp;PLCU_LIST Master Planet=#93 #4910 #6889 #1171 #3753 #6265 #87 #94 #88 #1170 #927 #852 #2505 #3006 #5380 #9381 #9629 #9029
&amp;PLCU_BALLYBRAN Master Planet=#93
&amp;MAKE_PLCU Master Planet=$make plcu type=* name=* leader=*:@swi hasflag(%#, wizard)=1,{@swi [gt(words(%0),0)]+[gt(words(%1),0)]+[gt(words(%2),0)]=1+1+1,{@swi words(v(plcu_[edit(edit(s(capstr(%1)), %b, $), ', #)]))=0,{@swi type(num(*%2)) = PLAYER,{@create [capstr(%1)] PLCU;&amp;place_name [capstr(%1)] PLCU=edit(edit(s(capstr(%1)), %b, $), ', #);&amp;place_type [capstr(%1)] PLCU=%0;&amp;time_plcu_created [capstr(%1)] PLCU=time();&amp;leader [capstr(%1)] PLCU=num(*%2);&amp;builder_char [capstr(%1)] PLCU=num(*%2);&amp;visitors_via_shuttle [capstr(%1)] PLCU=0;&amp;access [capstr(%1)] PLCU=restricted;&amp;allowed_access [capstr(%1)] PLCU=num(*%2);&amp;disallowed_access [capstr(%1)] PLCU=all;&amp;broadcast_permissions [capstr(%1)] PLCU=all;&amp;weather_state [capstr(%1)] PLCU=inactive;&amp;info [capstr(%1)] PLCU=%tTo be provided by [name(*%2)];&amp;greeting [capstr(%1)] PLCU=Welcome to %0 %1.;&amp;current_season [capstr(%1)] PLCU=0;&amp;months_into_season [capstr(%1)] PLCU=0;&amp;season_0_length [capstr(%1)] PLCU=0;&amp;season_1_length [capstr(%1)] PLCU=0;&amp;season_2_length [capstr(%1)] PLCU=0;&amp;season_3_length [capstr(%1)] PLCU=0;&amp;season_0_intensity [capstr(%1)] PLCU=1;&amp;season_1_intensity [capstr(%1)] PLCU=1;&amp;season_2_intensity [capstr(%1)] PLCU=1;&amp;season_3_intensity [capstr(%1)] PLCU=1;&amp;broadcast_prefix [capstr(%1)] PLCU=You hear a quiet tone on your wrist unit, and then a voice says,;@set [capstr(%1)] PLCU=quiet;@set [capstr(%1)] PLCU=dark;@set [capstr(%1)] PLCU=sticky;@parent [capstr(%1)] PLCU=#99;&amp;plcu_[edit(edit(s(capstr(%1)), %b, $), ', #)] [v(master_planet)]=con(me);&amp;plcu_list [v(master_planet)]=[v(plcu_list)] [con(me)];@pemit %#=PLCU created for %0 [capstr(%1)].%bmake sure and set &amp;sa_[edit(edit(s(capstr(%1)), %b, $), ', #)] #85=the dbref of the room which is directly linked to space and make sure that there is an exit from space (#76) to that room with a name of %1. Also, make sure and lock the exit from %1 into space to =SECURED_TYPE:shuttle;@trigger v(innocent_gadget)/link_and_send_home=con(me), num(*%2)},{@pemit %#=[capstr(%2)] is not a player.}},{@pemit %#=There is already a PLCU for that %0.}},{@pemit %#=You failed to enter at least one of the three required parameters. The command syntax is: make plcu type=&lt;type> name=&lt;name> leader=&lt;name>.}},{@pemit %#=You are not authorized to use this command.}
&amp;MASTER_PLANET Master Planet=#99
&amp;DESTROY_PLCU Master Planet=$destroy plcu for *:@swi hasflag(%#, wizard)=1,{@swi words(v(plcu_[edit(edit(s(capstr(%0)), %b, $), ', #)]))=0,{@pemit %#=There is not currently a PLCU for [capstr(%0)].},{&amp;plcu_list [v(master_planet)]=remove(v(plcu_list), v(plcu_[edit(edit(s(capstr(%0)), %b, $), ', #)]));@dest [v(plcu_[edit(edit(s(capstr(%0)), %b, $), ', #)])];&amp;plcu_[edit(edit(s(capstr(%0)), %b, $), ', #)] [v(master_planet)];@pemit %#=The PLCU for [capstr(%0)] has been destroyed.}},{@pemit %#=You are not authorized to use this command.}
&amp;SET_INFO Master Planet=$set info*=*:@swi u(admin_privs, %#)=yes,{@swi words(%1)=0,{&amp;info me=;@pemit %#=Information cleared for [v(place_type)]%b[edit(secure(v(place_name)), #, ')].},{&amp;info me=%1;@pemit %#=Information set for [v(place_type)]%b[edit(secure(v(place_name)), #, ')].}},{@pemit %#=You are not authorized to use this command.}
&amp;CLEAR_INFO Master Planet=$clear info*:@swi u(admin_privs, %#)=yes,{&amp;info me=;@pemit %#=Information cleared for [v(place_type)][edit(secure(v(place_name)), #, ')].},{@pemit %#=You are not authorized to use this command.}
&amp;EDIT_INFO Master Planet=$edit info*=*/*:@swi u(admin_privs, %#)=yes,{@swi %1=$,{@swi words(v(info))=0,{&amp;info me=%2},{@edit me/info=%1, {%2}}},^,{@swi words(v(info))=0,{&amp;info me=%2},{@edit me/info=%1, {%2}}},{@edit me/info={%1},{%2}};@trigger me/cleanup_edit_info=%#},{@pemit %#=You are not authorized to use this command.}
&amp;CLEANUP_EDIT_INFO Master Planet=@swi words(v(info))=0,{@pemit %0=There is no information currently registered for%b[edit(secure(v(place_name)), #, ')].},{@pemit %0=The information for [v(place_type)]%b[edit(secure(v(place_name)), #, ')] now reads:%b%b[s(v(info))]}
&amp;DISPLAY_INFO Master Planet=$display info*:@swi u(admin_privs, %#)=yes,{@swi words(v(info))=0,{@pemit %#=There is no information currently registered for%b[edit(secure(v(place_name)), #, ')].},{@pemit %#=The information for [v(place_type)]%b[edit(secure(v(place_name)), #, ')]%b reads:%b%b[s(v(info))]}},{@pemit %#=You are not authorized to use this command.}
&amp;DISPLAY_LANDING_FEE Master Planet=$display landing fee*:@swi u(admin_privs, %#)=yes,{@swi words(v(landing_fee))=0,{@pemit %#=There is no landing fee currently registered for%b[edit(secure(v(place_name)), #, ')].},{@pemit %#=The landing fee per person to reach [v(place_type)]%b[edit(secure(v(place_name)), #, ')] is set to:%b%b[switch(v(landing_fee),0,none,[v(landing_fee)] credits.)]}},{@pemit %#=You are not authorized to use this command.}
&amp;SET_LANDING_FEE Master Planet=$set landing fee*=*:@swi u(admin_privs, %#)=yes,{@swi words(%1)=0,{@pemit %#=Please specify a landing fee of greater than zero, or the keyword 'none'.},{@swi %1=none,{&amp;landing_fee me=0;@pemit %#=The landing fee has been set to none.},{@swi gt(%1, 0)=1,{&amp;landing_fee me=%1;@pemit %#=The landing fee per person for%b[edit(secure(v(place_name)), #, ')]%bhas been set to %1 credits.},{@pemit %#=Please specify a landing fee of greater than zero, or the keyword 'none'.}}}},{@pemit %#=You are not authorized to use this command.}
&amp;ADD_BROADCAST_PERMISSION Master Planet=$add broadcast per*=*:@swi u(admin_privs, %#)=yes,{@swi words(%1)=0,{@pemit %#=Please specify a list of people that you would like to add to the list of who has broadcast permissions on%b[edit(secure(v(place_name)), #, ')]%b or the keyword 'all'.},{@swi %1=all,{&amp;broadcast_permissions me=all;@pemit %#=Broadcast permissions are available for all persons on%b[edit(secure(v(place_name)), #, ')].},{@swi member(v(broadcast_permissions), all)=0,{&amp;broadcast_permissions me=[v(broadcast_permissions)] [iter(%1, num(*##))];&amp;broadcast_permissions me=remove(setunion(v(broadcast_permissions), v(broadcast_permissions)), #-1);@pemit %#=Broadcast permissions added.},{@pemit %#=The broadcast permissions are already set to 'all'.}}}},{@pemit %#=You are not authorized to use this command.}
&amp;REMOVE_BROADCAST_PERMISSION Master Planet=$remove broadcast per*=*:@swi u(admin_privs, %#)=yes,{@swi words(%1)=0,{@pemit %#=Please specify a list of people that you would like to%bremove from the list of who has broadcast permissions on%b[edit(secure(v(place_name)), #, ')] or the keyword 'all'.},{@swi %1=all,{&amp;broadcast_permissions me;@pemit %#=All broadcast permissions have been revoked.},{@dolist setunion(%1, %1)={@swi member(v(broadcast_permissions), num(*##))=0,{@pemit %#=[capstr(##)] is not currently in the list of%bthose with broadcast permissions on [edit(secure(v(place_name)), #, ')].},{&amp;broadcast_permissions me=remove(v(broadcast_permissions), num(*##))};@swi member(setunion(%1, %1), ##)=words(setunion(%1, %1)),{@pemit %#=Command completed.}}}}},{@pemit %#=You are not authorized to use this command.}
&amp;DISPLAY_BROADCAST_PERMISSION Master Planet=$display broadcast per*:@swi u(admin_privs, %#)=yes,{@swi words(v(broadcast_permissions))=0,{@pemit %#=No one has broadcast permissions for%b[edit(secure(v(place_name)), #, ')].},{@swi v(broadcast_permissions)=all,{@pemit %#=Everyone is allowed to do broadcasts on%b[edit(secure(v(place_name)), #, ')].},{@pemit %#=The people with broadcast permissions on%b[edit(secure(v(place_name)), #, ')] are:%b%b[iter(v(broadcast_permissions), name(##))]}}},{@pemit %#=You are not authorized to use this command.}
&amp;SET_BROADCAST_PREFIX Master Planet=$set broadcast pre*=*:@swi u(admin_privs, %#)=yes,{@swi words(%1)=0,{&amp;broadcast_prefix me=;@pemit %#=Broadcast prefix cleared for [v(place_type)]%b[edit(secure(v(place_name)), #, ')].},{&amp;broadcast_prefix me=%1;@pemit %#=Broadcast prefix set for [v(place_type)]%b[edit(secure(v(place_name)), #, ')].}},{@pemit %#=You are not authorized to use this command.}
&amp;EDIT_BROADCAST_PREFIX Master Planet=$edit broadcast pre*=*/*:@swi u(admin_privs, %#)=yes,{@swi %1=$,{@swi words(v(broadcast_prefix))=0,{&amp;broadcast_prefix me=%2},{@edit me/broadcast_prefix=%1, {%2}}},^,{@swi words(v(broadcast_prefix))=0,{&amp;broadcast_prefix me=%2},{@edit me/broadcast_prefix=%1, {%2}}},{@edit me/broadcast_prefix={%1},{%2}};@trigger me/cleanup_edit_broadcast_prefix=%#},{@pemit %#=You are not authorized to use this command.}
&amp;CLEANUP_EDIT_BROADCAST_PREFIX Master Planet=@swi words(v(broadcast_prefix))=0,{@pemit %0=There is no broadcast prefix currently registered for%b[edit(secure(v(place_name)), #, ')].},{@pemit %0=The broadcast prefix for [v(place_type)]%b[edit(secure(v(place_name)), #, ')]%b now reads:%b%b[s(v(broadcast_prefix))]}
&amp;DISPLAY_BROADCAST_PREFIX Master Planet=$display broadcast pre*:@swi u(admin_privs, %#)=yes,{@swi words(v(broadcast_prefix))=0,{@pemit %#=There is no broadcast prefix currently registered for%b[edit(secure(v(place_name)), #, ')].},{@pemit %#=The broadcast prefix for [v(place_type)]%b[edit(secure(v(place_name)), #, ')]%b reads:%b%b[s(v(broadcast_prefix))]}},{@pemit %#=You are not authorized to use this command.}
&amp;DISPLAY_ROOM_LIST Master Planet=$display room*:@swi u(admin_privs, %#)=yes,{@swi words(v(room_list))=0,{@pemit %#=There are no rooms currently registered for%b[edit(secure(v(place_name)), #, ')].},{@pemit %#=The rooms currently registered for [v(place_type)]%b[edit(secure(v(place_name)), #, ')] are:%b%b[v(room_list)]}},{@pemit %#=You are not authorized to use this command.}
&amp;ADD_ROOM Master Planet=$add room*=*: @swi u(admin_privs, %#)=yes, {@swi words(%1)=0, {@pemit %#=Please specify a list of rooms that you would like to add to the list of rooms for [edit(secure(v(place_name)), #, ')].}, {@dolist setunion(%1, )= {@swi type(##)=ROOM, {@swi owner(##)=%#, {&amp;room_list me=setunion(v(room_list) ##, ); @trigger v(innocent_gadget)/add_room_origin=##, num(me)}, {@pemit %#=## is not owned by you. You can only 'add room' rooms that you own.}}, {@pemit %#=## is not a room.}}; @swi 1 = 1, {@swi 2 = 2, {@swi 3 = 3, {@pemit %#=Command completed.}}}}}, {@pemit %#=You are not authorized to use this command.}
&amp;REMOVE_ROOM Master Planet=$remove room*=*:@swi u(admin_privs, %#)=yes,{@swi words(%1)=0,{@pemit %#=Please specify a list of rooms that you would like to%bremove from the list of rooms for [edit(secure(v(place_name)), #, ')].},{@dolist setunion(%1, %1)={@swi member(v(room_list), ##)=0,{@pemit %#=## is not currently in the list of%brooms for [edit(secure(v(place_name)), #, ')].},{&amp;room_list me=remove(v(room_list), ##);@trigger v(innocent_gadget)/remove_room_origin=##};@swi member(setunion(%1, %1), ##)=words(setunion(%1, %1)),{@pemit %#=Command completed.}}}},{@pemit %#=You are not authorized to use this command.}
&amp;MASTER_ROOM Master Planet=#123
&amp;PLCU_HELP Master Planet=$plcu help:@swi u(admin_privs, %#)=yes,{@pemit %#=%r[get(v(master_room)/equal_line)]%rPLCU Help (Planet/Moon Leader Command Unit)%r[get(v(master_room)/dash_line)]%r%rThere are several commands provided to help you manage the administrative tasks involved in being the leader of a Planet or Moon.%r%rHelp categories:%b%baccess, broadcasts, greeting, information, landing fees, %r[space(18)]rooms, summary, weather[switch(hasflag(%#, wizard),1,{, wiz})]%r%rTo obtain specific information about a category, use 'plcu help &lt;category>'%r[get(v(master_room)/equal_line)]%r},{@pemit %#=You are not authorized to use this command.}
&amp;PLCU_CATEGORIES Master Planet=access information greeting broadcasts rooms landing_fees wiz summary summary2 summary3 weather
&amp;PLCU_HELP_CATEGORY Master Planet=$plcu help *:@swi match(v(plcu_categories), edit(%0, %b, _)) = 0,{@pemit %#=Sorry, there is no help text for PLCU category '%0'.},@pemit %#=%r[get(v(master_room)/equal_line)]%rHelp text for PLCU category '%0':%r[get(v(master_room)/dash_line)]%r%r[s(v([edit(%0, %b, _)]_HELP))]%r%r[get(v(master_room)/equal_line)]%r
&amp;INFORMATION_HELP Master Planet=Each planet/moon Leader can set a block of text, recommended length, one or two paragraphs, not to exceed 24 lines, which can be used to explain a little about the planet and why it is an attractive place to visit. Try to give the reader a feeling for what activites they can expect to have available if they come to visit your planet/moon. Think of it as an advertisement of sorts, you are trying to attract visitors. This information is made available to all via the +computer global command.%r%r%b%bset info=&lt;text>%r%b%bclear info%r%b%bedit info=&lt;old>/&lt;new>%r%b%bdisplay info
&amp;GREETING_HELP Master Planet=Each planet/moon Leader can set a short message which gets shown to the occupants of a Shuttlecraft as it approaches their planet/moon to land. This should be something that seems reasonable for a Shuttle pilot to pass along to passengers, perhaps an indication of the weather, or places of interest to check out.%r%r%b%bset greeting=&lt;greeting>%r%b%bclear greeting%r%b%bedit greeting=&lt;old>/&lt;new>%r%b%bdisplay greeting
&amp;LANDING_FEES_HELP Master Planet=Each planet/moon Leader can set the number of credits that each person is charged to get a ride via Shuttle to their planet/moon. This money is collected at the Shuttle origination site, and is transferred to the appropriate planet/moon Leader in a timely fashion. Fees charged may possibly get to be competitve between the different planets/moons, but keep in mind that IC (In Character), members of the Heptite Guild are desperate to get off Ballybran regularly, and it's typically not free.%r%r%b%bset landing fee=&lt;number> or none%r%b%bdisplay landing fee
&amp;ROOMS_HELP Master Planet=Each planet/moon Leader can register a list of rooms (dbrefs) that are on their planet/moon. This information is used for a couple things, a) The global command, +where am i, uses it when trying to determine which planet/moon the invoker is on. If the rooms are registered, this becomes a possible task. b) There will be a broadcast capability per planet/moon. It would be silly to be able to broadcast a message over all planets/moons, so the broadcast capability is totally under the control of the planet leader. Registering which rooms are associated with a planet allows this planet-wide broadcasting capability. (see plcu help broadcasts for more info)%r%r%b%badd room=&lt;list of rooms>%r%b%bremove room=&lt;list of rooms>%r%b%bdisplay room list
&amp;BROADCASTS_HELP Master Planet=Each planet/moon Leader decides a) if there is a planet-wide broadcast capability at all for their planet, b) who is allowed to use it, c) what the text is that comes out before a broadcast, ie, 'Your wrist unit crackles and you hear ' or some such thing. The rooms where the broadcast messages are heard, are the rooms that have been registered by the leader via 'add room=&lt;room list>'. Conceivably, they could add all rooms on their planet, or only the 'public' ones. It is totally under the Leader's control.%r%r%b%badd broadcast permission=&lt;list of names> or all%r%b%bremove broadcast permission=&lt;list of names> or all%r%b%bdisplay broadcast permissions%r%b%bset broadcast prefix=&lt;text>%r%b%bedit broadcast prefix=&lt;old>/&lt;new>%r%b%bdisplay broadcast prefix
