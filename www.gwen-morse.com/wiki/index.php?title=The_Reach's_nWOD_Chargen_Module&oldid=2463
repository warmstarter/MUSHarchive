&amp;TRIGGER_STAT/ADD-GOBLIN CharGen Module=@eval [setq(E,edit(%1,%b,_))][setq(D,statlist(GOBLIN))][pemit(%0,switch(0,not(match(%qd,%qe)),That GOBLIN has already been checked in to the system.,eq(u(F_LETTERS,%qe),0),Goblin contract names may only contain letters%, underscores and hyphens.,[set(whichclass(%qe),SC-%qe:Goblin)][set(#89,LIST_GOBLIN:[setunion(%qd,%qe)])]You check in a new goblin contract:&#160;%qe%rYou can now check in the level with &quot;%xhstat/aspect GOBLIN/level=%qe/&lt;level&gt;%xn&quot;.))]

&amp;TRIGGER_STAT/ASPECT-GOBLIN/LEVEL CharGen Module=@eval [setq(Z,edit(%1,%b,_))][setq(E,edit(%2,%b,_))][setq(Q,statfull(GOBLIN,%qz))][pemit(%0,switch(0,statnum(GOBLIN,%qz),I can't find that goblin contract.,isrepeat(GOBLIN,%qz),mall(GOBLIN,%qz),match(1 2 3 4 5,%2),The goblin contract's level should be a whole number between 1 and 5.,[set(#2673,LEVEL-%qq:%2)]You set the level of the&#160;%qq goblin contract to &quot;%2&quot;.))]

&amp;TRIGGER_STAT/REM-GOBLIN CharGen Module=@eval [setq(E,edit(%1,%b,_))][setq(F,statfull(GOBLIN,%qe))][setq(D,statlist(GOBLIN))][pemit(%0,switch(0,statnum(GOBLIN,%qe),That goblin contract has not been checked in to the system.,isrepeat(GOBLIN,%qe),mall(GOBLIN,%qe),[set(whichclass(%qe),SC-%qf:)][set(#89,LIST_GOBLIN:[setdiff(%qd,%qf)])][set(#2673,LEVEL-%qf:)]You remove the &quot;%qf&quot; goblin contract and its associated level.))]

&amp;TRIGGER_SET-GOBLIN CharGen Module=@eval [setq(E,edit(%2,%b,_))][setq(T,if(strmatch(%qe,!*),delete(%qe,0,1),%qe))][setq(F,statfull(goblin,%qt))][setq(D,get(%1/_GOBLIN))][pemit(%0,switch(0,statnum(goblin,%qt),I can't find that goblin contract.,isrepeat(goblin,%qt),mall(goblin,%qt),not(strmatch(%2,!*)),if(match(%qd,%qf),[set(%1,_GOBLIN:[setdiff(%qd,%qf)])]You remove the&#160;%qf goblin contract from [name(%1)]'s sheet.,[name(%1)] doesn't have the&#160;%qf goblin contract on [poss(%1)] sheet.),if(match(%qd,%qf),[name(%1)] already has the&#160;%qf goblin contract.,[set(%1,_GOBLIN:[setunion(%qd,%qf)])]You add the&#160;%qf goblin contract to [name(%1)]'s sheet.)))]

&amp;TEMPLATE-PROMETHEAN CharGen Module=[ulocal(SHEET-HEADER,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-BASICS-PROMETHEAN,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-ATTRIBUTES,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-SKILLS,%0)]%r%xr[repeat(-,77)]%xn[if(hasattr(%0,_MERITS),%r[ulocal(SHEET-MERITS,%0)]%r%xr[repeat(-,77)]%xn)][if(hasattr(%0,_TRANSMUTATIONS),%r[ulocal(SHEET-TRANSMUTATIONS,%0)]%r%xr[repeat(-,77)]%xn)][if(hasattr(%0,_BESTOWMENTS),%r[ulocal(SHEET-BESTOWMENT,%0)]%r%xr[repeat(-,77)]%xn)][if(hasattr(%0,_RITUALS),%r[ulocal(SHEET-RITUALS,%0)]%r%xr[repeat(-,77)]%xn)]%r[ulocal(SHEET-ADVANTAGES-CHANGELING,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-HEALTH-PROMETHEAN,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-FOOTER,%0)]

&amp;SHEET-BASICS-PROMETHEAN CharGen Module=[ljust(Template: Promethean,25)] [ljust(Concept: [default(%0/_CONCEPT,&lt;unset&gt;)],52)]%r[table(Humanity: [default(%0/_HUMANITY,7)]|Virtue: [default(%0/_VIRTUE,&lt;unset&gt;)]|Vice: [default(%0/_VICE,&lt;unset&gt;)]|Refinement: [edit(default(%0/_REFINEMENT,&lt;unset&gt;),_,%b)]|Lineage: [edit(default(%0/_LINEAGE,&lt;unset&gt;),_,%b)]|Family: [edit(default(%0/_FAMILY,&lt;unset&gt;),_,%b)]|[ljust(Birthdate: [if(hasattr(%0,_DOB),[timefmt($d $b $Y,convtime(setr(W,get(%0/_DOB))))],&lt;unset&gt;)],25)]|[ljust(Age: [if(hasattr(%0,_DOB),ulocal(F_AGE,[extract(%qw,2,2)] [last(%qw)]),&lt;unset&gt;)],25)]|Experience: [first(setr(X,default(%0/_XP,0.0 0.0 0.0)))]%/[last(%qx)],25,77,|)]

