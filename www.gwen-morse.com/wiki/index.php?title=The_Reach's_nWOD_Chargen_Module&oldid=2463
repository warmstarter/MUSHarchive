&amp;TRIGGER_STAT/ADD-BLESSING CharGen Module=[setq(E,edit(%1,%b,_))][setq(D,statlist(blessing))][pemit(%0,switch(0,not(match(%qd,%qe)),That blessing has already been checked in to the system.,eq(u(F_LETTERS,%qe),0),blessing names may only contain letters%, underscores and hyphens.,[set(whichclass(%qe),SC-%qe:blessing)][set(#89,LIST_blessing:[setunion(%qd,%qe)])]You check in a new blessing:&#160;%qe%rYou can now check in its aspects&#160;%(cost and access%).))]

&amp;TRIGGER_STAT/ASPECT-BLESSING/LEVEL CharGen Module=@eval [setq(Z,edit(%1,%b,_))][setq(E,edit(%2,%b,_))][setq(Q,statfull(blessing,%qz))][pemit(%0,switch(0,statnum(blessing,%qz),I can't find that blessing.,isrepeat(blessing,%qz),mall(blessing,%qz),match(1 2 3 4 5,%2),The blessing's level should be a whole number between 1 and 5.,[set(#2673,LEVEL-%qq:%2)]You set the level of the&#160;%qq blessing to &quot;%2&quot;.))]

&amp;TRIGGER_STAT/REM-BLESSING CharGen Module=@eval [setq(E,edit(%1,%b,_))][setq(F,statfull(blessing,%qe))][setq(D,statlist(blessing))][pemit(%0,switch(0,statnum(blessing,%qe),That blessing has not been checked in to the system.,isrepeat(blessing,%qe),mall(blessing,%qe),[set(whichclass(%qe),SC-%qf:)][set(#89,LIST_blessing:[setdiff(%qd,%qf)])][set(#2672,COST-%qf:)][set(#2681,ACCESS-%qf:)]You remove the &quot;%qf&quot; discipline and its associated aspects.))]

&amp;TRIGGER_STAT/ASPECT-BLESSING/COST CharGen Module=@eval [setq(Z,edit(%1,%b,_))][setq(Q,statfull(blessing,%qz))][setq(X,if(strmatch(%2,*-*),lnum(before(%2,-),after(%2,-)),%2))][pemit(%0,switch(0,statnum(blessing,%qz),I can't find that blessing.,isrepeat(blessing,%qz),mall(blessing,%qz),eq(u(F_NUMBERS,%qx),0),The cost%(s%) of a blessing should be a number&#160;%(e.g. 2%)%, a space-separated list of numbers&#160;%(e.g. 1 2 4%)%, or a range&#160;%(e.g. 1-3%).,[set(#2672,COST-%qq:%2)]You set the cost%(s%) for the&#160;%qq blessing to &quot;%2&quot;.))]

