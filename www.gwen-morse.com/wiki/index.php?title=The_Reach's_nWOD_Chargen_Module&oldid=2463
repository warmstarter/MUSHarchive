&amp;SHEET-ADVANTAGES-PROMETHEAN CharGen Module=[setq(A,default(%0/_ATTRIBUTES,1 1 1 1 1 1 1 1 1))][setq(M,default(%0/_ATMODIFIED,0 0 0 0 0 0 0 0 0))][center(A D V A N T A G E S,77)]%r%xr[repeat(-,77)]%xn%r[table([ljust(Defense,17,.)] [setq(D,ulocal(SCORE-DEFENSE,%0))][rjust(first(%qd),2)]%([rjust(last(%qd),2)]%)|[ljust(Initiative,17,.)] [setq(I,ulocal(SCORE-INITIATIVE,%0))][rjust(first(%qi),2)]%([rjust(last(%qi),2)]%)|[ljust(Size,17,.)] [setq(S,ulocal(SCORE-SIZE,%0))][rjust(first(%qs),2)]%([rjust(last(%qs),2)]%)|[ljust(Speed,17,.)] [setq(P,ulocal(SCORE-SPEED,%0))][rjust(first(%qp),2)]%([rjust(last(%qp),2)]%)|[ljust(Curr Willpower,17,.)] [setq(W,ulocal(SCORE-WILLPOWER,%0))][rjust(first(%qw),2)]%([rjust(extract(%qw,2,1),2)]%)|[ljust(Perm Willpower,17,.)] [rjust(extract(%qw,3,1),2)]%([rjust(last(%qw),2)]%),25,77,|)]

&amp;SHEET-TRANSMUTATIONS CharGen Module=[setq(D,get(%0/_TRANSMUTATIONS))][center(T R A N S M U T A T I O N S,77)]%r%xr[repeat(-,77)]%xn%r[table(sort(iter(%qd,if(hasattr(#2682,POWER-[before(%i0,:)]_1),iter(lnum(1,after(%i0,:)),iter(get(#2682/POWER-[before(%i1,:)]_%i0),edit([before(%i2,:)]&#160;%i1&#160;%(%i0%),_,%b),|,|),,|),[edit(before(%i0,:),_,%b)] [after(%i0,:)]),,|),a,|,|),38,77,|,,.)]

&amp;PEMIT_SECTION-TRANSMUTATIONS CharGen Module=[if(hasattr(%0,_TRANSMUTATIONS),[ulocal(SHEET-HEADER,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-TRANSMUTATIONS,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-FOOTER,%0)],[name(%0)] has no Transmutations.)]

&amp;TRIGGER_SET-TRANSMUTATION CharGen Module=@eval [setq(E,edit(%2,%b,_))][setq(T,if(strmatch(%qe,!*),delete(before(%qe,:),0,1),before(%qe,:)))][setq(F,statfull(Transmutation,%qt))][setq(V,after(%qe,:))][setq(D,get(%1/_TransmutationS))][setq(Q,grab(%qd,%qf:*))][pemit(%0,switch(0,statnum(Transmutation,%qt),I can't find that Transmutation.,isrepeat(Transmutation,%qt),mall(Transmutation,%qt),not(strmatch(%2,!*)),if(setr(M,match(%qd,%qf:*)),[set(%1,_TransmutationS:[ldelete(%qd,%qm)])]You remove the&#160;%qf Transmutation from [name(%1)]'s sheet.,[name(%1)] doesn't have the&#160;%qf Transmutation on [poss(%1)] sheet.),match(lnum(1,setr(L,score(%1,limit))),%qv),Please choose a dot value between 1 and&#160;%ql.,not(setr(Z,match(%qd,%qf:*))),[set(%1,_TransmutationS:[replace(%qd,%qz,%qf:%qv)])]You set [name(%1)]'s&#160;%qf to&#160;%qv.,[set(%1,_TransmutationS:[trim(%qd&#160;%qf:%qv)])]You set [name(%1)]'s&#160;%qf to&#160;%qv.))]

&amp;TRIGGER_RAISE-TRANSMUTATION CharGen Module=@eval [setq(E,edit(%2,%b,_))][setq(F,statfull(Transmutation,%qe))][setq(A,get(%1/_TransmutationS))][setq(Q,if(match(%qa,%qf:*),after(grab(%qa,%qf:*),:),0))][pemit(%0,switch(0,setr(M,statnum(Transmutation,%qe)),I can't find that Transmutation.,isrepeat(Transmutation,%qe),mall(Transmutation,%qe),lt(%qq,setr(L,score(%1,limit))),[name(%1)]'s&#160;%qf is already at the limit of&#160;%ql dots.,[set(%1,_TransmutationS:[replace(%qa,%qm,%qf:[setr(I,inc(%qq))])])]You raise [name(%1)]'s&#160;%qf Transmutation to&#160;%qi.))]

&amp;TRIGGER_LOWER-TRANSMUTATION CharGen Module=@eval [setq(E,edit(%2,%b,_))][setq(F,statfull(Transmutation,%qe))][setq(A,get(%1/_TransmutationS))][setq(Q,if(match(%qa,%qf:*),after(grab(%qa,%qf:*),:),0))][pemit(%0,switch(0,setr(M,statnum(Transmutation,%qe)),I can't find that Transmutation.,isrepeat(Transmutation,%qe),mall(Transmutation,%qe),gt(%qq,1),[name(%1)]'s&#160;%qf is already at 1 dot&#160;%(&quot;use sheet/set [name(%1)]/Transmutation=!%qf&quot; to remove the Transmutation from [poss(%1)] sheet%).,[set(%1,_TransmutationS:[replace(%qa,%qm,%qf:[setr(I,dec(%qq))])])]You lower [name(%1)]'s&#160;%qf Transmutation to&#160;%qi.))]

&amp;TRIGGER_STAT/ADD-TRANSMUTATION CharGen Module=[setq(E,edit(%1,%b,_))][setq(D,statlist(Transmutation))][pemit(%0,switch(0,not(match(%qd,%qe)),That Transmutation has already been checked in to the system.,eq(u(F_LETTERS,%qe),0),Transmutation names may only contain letters%, underscores and hyphens.,[set(whichclass(%qe),SC-%qe:Transmutation)][set(#89,LIST_TRANSMUTATION:[setunion(%qd,%qe)])]You check in a new Transmutation:&#160;%qe%rYou can now check in its powers&#160;%(where applicable%) with &quot;%xhstat/aspect disc/power=%qe/&lt;level&gt;:&lt;power name&gt;%xn&quot; for each power.))]

&amp;TRIGGER_STAT/REM-TRANSMUTATION CharGen Module=@eval [setq(E,edit(%1,%b,_))][setq(F,statfull(Transmutation,%qe))][setq(D,statlist(Transmutation))][pemit(%0,switch(0,statnum(Transmutation,%qe),That Transmutation has not been checked in to the system.,isrepeat(Transmutation,%qe),mall(Transmutation,%qe),[set(whichclass(%qe),SC-%qf:)][set(#89,LIST_TRANSMUTATION:[setdiff(%qd,%qf)])][wipe(#2682/POWER-%qf_*)]You remove the &quot;%qf&quot; Transmutation and its associated powers.))]

&amp;TRIGGER_STAT/ASPECT-TRANSMUTATION/POWER CharGen Module=@eval [setq(Z,edit(%1,%b,_))][setq(F,statfull(Transmutation,%qz))][setq(E,edit(%2,%b,_))][setq(B,before(%qe,:))][setq(A,after(%qe,:))][pemit(%0,switch(0,statnum(Transmutation,%qz),I can't find that Transmutation.,isrepeat(Transmutation,%qz),mall(Transmutation,%qz),match(1 2 3 4 5,%qb),I can't find that Transmutation level&#160;%(%qb%).,[set(#2682,POWER-%qf_%qb:%qa)]You set&#160;%qf's level&#160;%qb power to &quot;%qa&quot;.))]

