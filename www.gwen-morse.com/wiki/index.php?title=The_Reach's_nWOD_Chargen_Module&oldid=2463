&amp;FUNC_ROLLSTAT CharGen Module=switch(0,match(perm pump,%3),#-6,isdbref(setr(N,locate(%#,%0,amnpP))),#-1,or(strmatch(owner(%#),%qn),isstaff(owner(%#))),#-2,iter(%1,[setq(8,edit(trim(%i0),%b,_))][setq(W,whichclass(%q8))][setq(Q,lattr(%qw/SC-%q8*))][setq(M,match(%qq,SC-%q8*))][setq(F,after(grab(%qq,SC-%q8*),SC-))][setq(K,get(%qw/SC-%qf))][switch(0,not(isnum(%q8)),if(strmatch(%q8,*.*),#-3,%q8),%qm,#-4,or(match(%qq,SC-%q8),eq(words(graball(%qq,SC-%q8*)),1)),#-5,not(strmatch(%qk:%3,Get:*)),#-7,not(strmatch(%qk:%3,AllVices:*)),ladd(iter(default(%qn/_VICE-POWERS,None:0),after(%i0,:))),not(strmatch(%qk:%3,Seven:*)),default(%qn/_%qf,7),not(strmatch(Score:perm,%qk:%3)),first(score(%qn,%qf)),not(strmatch(Score:pump,%qk:%3)),last(score(%qn,%qf)),not(strmatch(Willpower:perm,%qk:%3)),extract(score(%qn,willpower),3,1),not(strmatch(Willpower:pump,%qk:%3)),extract(score(%qn,willpower),4,1),not(strmatch(%qk:%3,Default:*)),[setq(I,Azoth Feral-Heart Gnosis Potency Primal-Urge Psyche Wyrd)][setq(J,Promethean Shifter Mage Vampire Werewolf Geist Changeling)][default(%qn/_%qf,if(strmatch(extract(%qj,match(%qi,%qf),1),default(%qn/_TEMPLATE,Mortal)),1,0))],not(strmatch(%qk:%3,*:perm)),first(statval(%qn,%qk,%qf)),not(strmatch(%qk:%3,*:pump)),last(statval(%qn,%qk,%qf)))],%2))

