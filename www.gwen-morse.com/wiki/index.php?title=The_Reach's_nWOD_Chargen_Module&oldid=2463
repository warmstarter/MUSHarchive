&amp;TRIGGER_RAISE-DISCIPLINE CharGen Module=@eval [setq(E,edit(%2,%b,_))][setq(F,statfull(discipline,%qe))][setq(A,get(%1/_DISCIPLINES))][setq(Q,if(match(%qa,%qf:*),after(grab(%qa,%qf:*),:),0))][pemit(%0,switch(0,setr(M,statnum(discipline,%qe)),I can't find that discipline.,isrepeat(discipline,%qe),mall(discipline,%qe),lt(%qq,setr(L,score(%1,limit))),[name(%1)]'s&#160;%qf is already at the limit of&#160;%ql dots.,[set(%1,_DISCIPLINES:[replace(%qa,%qm,%qf:[setr(I,inc(%qq))])])]You raise [name(%1)]'s&#160;%qf discipline to&#160;%qi.))]

