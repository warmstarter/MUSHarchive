&amp;CMD-STAT/ADD CharGen Module=$stat/add *=*:@pemit&#160;%#=switch(0,hasflag(%#,ROYALTY),Permission denied.,statnum(addstat,%0),I can't find that character sheet stat type., isrepeat(addstat,&#160;%0), mall(addstat,&#160;%0), not(hasattr(whichclass(%1), SC-[edit(%1,&#160;%b, _)])), A trait of type '[get(whichclass(%1)/SC-[edit(%1,&#160;%b, _)])]' already has that name., trigger(#89/TRIGGER_STAT/ADD-[statfull(addstat,%0)],%#,%1))

@set CharGen Module/CMD-STAT/ADD = no_parse

&amp;CMD-STAT/LIST CharGen Module=$stat/list*:@pemit&#160;%#=switch(0, strlen(setr(T, trim(first(%0, =)))), ulocal(PEMIT_STATLIST, all, rest(%0, =)), not(strmatch(%qt, all)), ulocal(PEMIT_STATLIST, all, rest(%0, =)), statnum(addstat,&#160;%qt), ulocal(PEMIT_STATLIST, all), isrepeat(addstat,&#160;%qt), mall(addstat,&#160;%qt), ulocal(PEMIT_STATLIST, statfull(addstat,&#160;%qt), rest(%0, =)))

&amp;PEMIT_STATLIST CharGen Module=[wheader(Stats of type '[titlestr(edit(lcstr(%0), _,&#160;%b))]')]%r[udefault(statlist-%0, u(statlist-all),&#160;%1)]%r[wfooter(if(words(%qc), words(%qc) stats listed, none found))]

&amp;F_LETTERS CharGen Module=strlen(strip(%0, ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz-_'()))

&amp;CMD-STAT/REM CharGen Module=$stat/rem *=*:@pemit&#160;%#=switch(0,hasflag(%#,WIZARD),Permission denied.,statnum(addstat,%0),I can't find that character sheet stat type.,isrepeat(addstat,%0),mall(addstat,%0),trigger(#89/TRIGGER_STAT/REM-[statfull(addstat,%0)],%#,%1))

@set CharGen Module/CMD-STAT/REM = no_parse

