&amp;TRIGGER_SET-RENOWN CharGen Module=@eval [setq(C, after(%2, /))][setq(E, edit(before(%2, /),&#160;%b, _))][setq(T, if(strmatch(%qe,&#160;!*), delete(before(%qe,&#160;:), 0, 1), before(%qe,&#160;:)))][setq(F, statfull(renown,&#160;%qt))][setq(V, after(%qe,&#160;:))][setq(D, get(%1/_RENOWN))][pemit(%0, switch(0, statnum(renown,&#160;%qt), I can't find that renown., isrepeat(renown,&#160;%qt), mall(renown,&#160;%qt), not(strmatch(%2,&#160;!*)), if(setr(M, match(%qd,&#160;%qf~%qv~[if(strlen(%qc),&#160;%qc, *)], |)), [set(%1, _renown:[ldelete(%qd,&#160;%qm, |)])]You remove the&#160;%qf (value:&#160;%qv[if(%qc,&#160;%b%(%qc%))]) renown from [name(%1)]'s sheet., [name(%1)] doesn't have the&#160;%qf (value:&#160;%qv[if(%qc,&#160;%b%(%qc%))]) renown on [poss(%1)] sheet.), match(setr(X, if(strmatch(setr(Z, get(#2672/COST-%qf)), *-*), lnum(before(%qz, -), after(%qz, -)),&#160;%qz)),&#160;%qv), The&#160;%qf renown must have a point value of [itemize(%qx, , or)]., [set(%1, _RENOWN:[if(%qd,&#160;%qd|)]%qf~%qv~%qc)]You set [name(%1)]'s&#160;%qf to&#160;%qv[if(%qc,&#160;%b%(%qc%))].))]

&amp;TRIGGER_RAISE-RENOWN CharGen Module=@eval [setq(E,edit(%2,%b,_))][setq(F,statfull(renown,%qe))][setq(A,get(%1/_renown))][setq(Q,if(match(%qa,%qf:*),after(grab(%qa,%qf:*),:),0))][pemit(%0,switch(0,setr(M,statnum(renown,%qe)),I can't find that renown.,isrepeat(renown,%qe),mall(renown,%qe),lt(%qq,setr(L,score(%1,limit))),[name(%1)]'s&#160;%qf is already at the limit of&#160;%ql dots.,[set(%1,_renown:[replace(%qa,%qm,%qf:[setr(I,inc(%qq))])])]You raise [name(%1)]'s&#160;%qf renown to&#160;%qi.))]

&amp;TRIGGER_LOWER-RENOWN CharGen Module=@eval [setq(E,edit(%2,%b,_))][setq(F,statfull(renown,%qe))][setq(A,get(%1/_renown))][setq(Q,if(match(%qa,%qf:*),after(grab(%qa,%qf:*),:),0))][pemit(%0,switch(0,setr(M,statnum(renown,%qe)),I can't find that renown.,isrepeat(renown,%qe),mall(renown,%qe),gt(%qq,1),[name(%1)]'s&#160;%qf is already at 1 dot&#160;%(&quot;use sheet/set [name(%1)]/renown=!%qf&quot; to remove the renown from [poss(%1)] sheet%).,[set(%1,_renown:[replace(%qa,%qm,%qf:[setr(I,dec(%qq))])])]You lower [name(%1)]'s&#160;%qf renown to&#160;%qi.))]

