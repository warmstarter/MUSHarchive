&amp;TRIG.APPROVE.IMMORTAL/PATCHWORK_PEOPLE CharGen Module=@eval u(fct.faction.set, Immortal, name(%0), Patchwork People);

&amp;TEMPLATE-WARDEN CharGen Module=[ulocal(SHEET-HEADER,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-BASICS-Warden,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-ATTRIBUTES,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-SKILLS,%0)]%r%xr[repeat(-,77)]%xn[if(hasattr(%0,_MERITS),%r[ulocal(SHEET-MERITS,%0)]%r%xr[repeat(-,77)]%xn)]%r[ulocal(SHEET-ADVANTAGES-Warden,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-HEALTH-Warden,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-FOOTER,%0)]

&amp;SHEET-BASICS-WARDEN CharGen Module=[ljust(Template: Warden,25)] [ljust(Concept: [default(%0/_CONCEPT,&lt;unset&gt;)],52)]%r[table(Morality: [default(%0/_Morality,7)]|Virtue: [default(%0/_VIRTUE,&lt;unset&gt;)]|Vice: [default(%0/_VICE,&lt;unset&gt;)]|Family: [default(%0/_Family,&lt;unset&gt;)]|Profession: [default(%0/_Profession,&lt;unset&gt;)]|[ljust(Birthdate: [if(hasattr(%0,_DOB),[timefmt($d $b $Y,convtime(setr(W,get(%0/_DOB))))],&lt;unset&gt;)],25)]|[ljust(Age: [if(hasattr(%0,_DOB),ulocal(F_AGE,[extract(%qw,2,2)] [last(%qw)]),&lt;unset&gt;)],25)]|Experience: [first(setr(X,default(%0/_XP,0.0 0.0 0.0)))]%/[last(%qx)],25,77,|)]

&amp;SHEET-ADVANTAGES-WARDEN CharGen Module=[setq(A,default(%0/_ATTRIBUTES,1 1 1 1 1 1 1 1 1))][setq(M,default(%0/_ATMODIFIED,0 0 0 0 0 0 0 0 0))][center(A D V A N T A G E S,77)]%r%xr[repeat(-,77)]%xn%r[table([ljust(Defense,17,.)] [setq(D,ulocal(SCORE-DEFENSE,%0))][rjust(first(%qd),2)]%([rjust(last(%qd),2)]%)|[ljust(Initiative,17,.)] [setq(I,ulocal(SCORE-INITIATIVE,%0))][rjust(first(%qi),2)]%([rjust(last(%qi),2)]%)|[ljust(Size,17,.)] [setq(S,ulocal(SCORE-SIZE,%0))][rjust(first(%qs),2)]%([rjust(last(%qs),2)]%)|[ljust(Speed,17,.)] [setq(P,ulocal(SCORE-SPEED,%0))][rjust(first(%qp),2)]%([rjust(last(%qp),2)]%)|[ljust(Curr Willpower,17,.)] [setq(W,ulocal(SCORE-WILLPOWER,%0))][rjust(first(%qw),2)]%([rjust(extract(%qw,2,1),2)]%)|[ljust(Perm Willpower,17,.)] [rjust(extract(%qw,3,1),2)]%([rjust(last(%qw),2)]%),25,77,|)]

&amp;SHEET-HEALTH-WARDEN CharGen Module=u(fct.sheet.health.display,&#160;%0, @@(no power stat), @@(no power pool), MINOR)

&amp;DATA.SUPERSTAT.MAX.WARDEN CharGen Module=6|5|5|6|5|5|6|5|5|5

&amp;TRIG.APPROVE.IMMORTAL/WARDEN CharGen Module=@eval u(fct.faction.set, Immortal, name(%0), Warden);

&amp;TEMPLATE-REBORN CharGen Module=[ulocal(SHEET-HEADER,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-BASICS-Reborn,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-ATTRIBUTES,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-SKILLS,%0)]%r%xr[repeat(-,77)]%xn[if(hasattr(%0,_MERITS),%r[ulocal(SHEET-MERITS,%0)]%r%xr[repeat(-,77)]%xn)]%r[ulocal(SHEET-ADVANTAGES-Reborn,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-HEALTH-Reborn,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-FOOTER,%0)]

