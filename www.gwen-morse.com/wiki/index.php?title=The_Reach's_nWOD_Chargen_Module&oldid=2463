&amp;SHEET-ARCANA CharGen Module=[setq(R,sort(get(%0/_ARCANA),a))][center(A R C A N A,77)]%r%xr[repeat(-,77)]%xn%r[table(iter(%qr,[ljust(edit(first(%i0,&#160;:),_,%b),34,.)] [rjust(after(%i0,:),2)],,|),38,77,|)]

&amp;PEMIT_SECTION-ARCANA CharGen Module=[if(hasattr(%0,_Arcana),[ulocal(SHEET-HEADER,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-Arcana,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-FOOTER,%0)],[name(%0)] has no Arcana.)]

&amp;TRIGGER_SET-ARCANA CharGen Module=@eval [setq(E,edit(%2,%b,_))][setq(T,if(strmatch(%qe,!*),delete(before(%qe,:),0,1),before(%qe,:)))][setq(F,statfull(Arcana,%qt))][setq(V,after(%qe,:))][setq(D,get(%1/_Arcana))][setq(Q,grab(%qd,%qf:*))][pemit(%0,switch(0,statnum(Arcana,%qt),I can't find that Arcana.,isrepeat(Arcana,%qt),mall(Arcana,%qt),not(strmatch(%2,!*)),if(setr(M,match(%qd,%qf:*)),[set(%1,_Arcana:[ldelete(%qd,%qm)])]You remove the&#160;%qf Arcana from [name(%1)]'s sheet.,[name(%1)] doesn't have the&#160;%qf Arcana on [poss(%1)] sheet.),match(lnum(1,setr(L,score(%1,limit))),%qv),Please choose a dot value between 1 and&#160;%ql.,not(setr(Z,match(%qd,%qf:*))),[set(%1,_Arcana:[replace(%qd,%qz,%qf:%qv)])]You set [name(%1)]'s&#160;%qf to&#160;%qv.,[set(%1,_Arcana:[trim(%qd&#160;%qf:%qv)])]You set [name(%1)]'s&#160;%qf to&#160;%qv.))]

&amp;TRIGGER_RAISE-ARCANA CharGen Module=@eval [setq(E,edit(%2,%b,_))][setq(F,statfull(Arcana,%qe))][setq(A,get(%1/_Arcana))][setq(Q,if(match(%qa,%qf:*),after(grab(%qa,%qf:*),:),0))][pemit(%0,switch(0,setr(M,statnum(Arcana,%qe)),I can't find that Arcana.,isrepeat(Arcana,%qe),mall(Arcana,%qe),lt(%qq,setr(L,score(%1,limit))),[name(%1)]'s&#160;%qf is already at the limit of&#160;%ql dots.,[set(%1,_Arcana:[replace(%qa,%qm,%qf:[setr(I,inc(%qq))])])]You raise [name(%1)]'s&#160;%qf Arcana to&#160;%qi.))]

&amp;TRIGGER_LOWER-ARCANA CharGen Module=@eval [setq(E,edit(%2,%b,_))][setq(F,statfull(Arcana,%qe))][setq(A,get(%1/_Arcana))][setq(Q,if(match(%qa,%qf:*),after(grab(%qa,%qf:*),:),0))][pemit(%0,switch(0,setr(M,statnum(Arcana,%qe)),I can't find that Arcana.,isrepeat(Arcana,%qe),mall(Arcana,%qe),gt(%qq,1),[name(%1)]'s&#160;%qf is already at 1 dot&#160;%(&quot;use sheet/set [name(%1)]/Arcana=!%qf&quot; to remove the Arcana from [poss(%1)] sheet%).,[set(%1,_Arcana:[replace(%qa,%qm,%qf:[setr(I,dec(%qq))])])]You lower [name(%1)]'s&#160;%qf Arcana to&#160;%qi.))]

&amp;TRIGGER_STAT/ADD-ARCANA CharGen Module=[setq(E,edit(%1,%b,_))][setq(D,statlist(Arcana))][pemit(%0,switch(0,not(match(%qd,%qe)),That Arcana has already been checked in to the system.,eq(u(F_LETTERS,%qe),0),Arcana names may only contain letters%, underscores and hyphens.,[set(whichclass(%qe),SC-%qe:Arcana)][set(#89,LIST_Arcana:[setunion(%qd,%qe)])]You check in a new Arcana:&#160;%qe%rYou can now check in its powers&#160;%(where applicable%) with &quot;%xhstat/aspect disc/power=%qe/&lt;level&gt;:&lt;power name&gt;%xn&quot; for each power.))]

&amp;TRIGGER_STAT/REM-ARCANA CharGen Module=@eval [setq(E,edit(%1,%b,_))][setq(F,statfull(Arcana,%qe))][setq(D,statlist(Arcana))][pemit(%0,switch(0,statnum(Arcana,%qe),That Arcana has not been checked in to the system.,isrepeat(Arcana,%qe),mall(Arcana,%qe),[set(whichclass(%qe),SC-%qf:)][set(#89,LIST_Arcana:[setdiff(%qd,%qf)])][wipe(#2682/POWER-%qf_*)]You remove the &quot;%qf&quot; Arcana and its associated powers.))]

&amp;PROVE-ARCANA CharGen Module=if(match(get(%0/_Arcana),&#160;%1:*), [moniker(%0)] has [art(%1)] &quot;%2&quot; Arcana of [statval(%0, Arcana,&#160;%1)]., [moniker(%0)] lacks the &quot;%2&quot; Arcana.)

&amp;SHEET-ROTES CharGen Module=[setq(M,sort(get(%0/_rotes),a,|,|))][center(R O T E S,77)]%r%xr[repeat(-,77)]%xn%r[iter(%qm,[ljust(titlestr(edit(first(%i0,~),_,%b)),34,.)] [rjust(extract(%i0,2,1,~),2)] [center(if(setr(N,titlestr(edit(last(%i0,~), +, |+|,&#160;%b, , |+|,&#160;%b+%b))),%(%qn%)),38)],|,%r)]

&amp;PEMIT_SECTION-ROTES CharGen Module=[if(hasattr(%0,_ROTES),[ulocal(SHEET-HEADER,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-Rotes,%0)]%r%xr[repeat(-,77)]%xn%r[ulocal(SHEET-FOOTER,%0)],[name(%0)] has no Rote.)]

&amp;TRIGGER_SET-ROTE CharGen Module=[setq(C,after(%2,/))][setq(E,edit(before(%2,/),%b,_))][setq(T,if(strmatch(%qe,!*),delete(before(%qe,:),0,1),before(%qe,:)))][setq(F,statfull(rote,%qt))][setq(V,after(%qe,:))][setq(D,get(%1/_Rotes))][pemit(%0,switch(0,statnum(rote,%qt),I can't find that rote.,isrepeat(rote,%qt),mall(rote,%qt),not(strmatch(%2,!*)),if(setr(M,match(%qd,%qf~*~*,|)),[set(%1,_Rotes:[ldelete(%qd,%qm,|)])]You remove the&#160;%qf rote from [name(%1)]'s sheet.,[name(%1)] doesn't have the&#160;%qf rote on [poss(%1)] sheet.),match(setr(X,if(strmatch(setr(Z,get(#2672/COST-%qf)),*-*),lnum(before(%qz,-),after(%qz,-)),%qz)),%qv),The&#160;%qf rote must have a point value of [itemize(%qx,,or)].,[set(%1,_Rotes:[if(%qd,%qd|)]%qf~%qv~%qc)]You set [name(%1)]'s&#160;%qf to&#160;%qv[if(%qc,%b%(%qc%))].))]

&amp;PROVE-ROTE CharGen Module=if(match(setr(G,get(%0/_roteS)),%1~*~*,|),[moniker(%0)] has a [extract(setr(H,grab(%qg,%1~*~*,|)),2,1,~)]-point &quot;%2&quot; rote[if(strlen(setr(I,extract(%qh,3,1,~))),%b%(%qi%))].,[moniker(%0)] lacks the &quot;%2&quot; rote.)

&amp;TRIGGER_STAT/ADD-ROTE CharGen Module=[setq(E,edit(%1,%b,_))][setq(D,statlist(rote))][pemit(%0,switch(0,not(match(%qd,%qe)),That rote has already been checked in to the system.,eq(u(F_LETTERS,%qe),0),rote names may only contain letters%, underscores and hyphens.,[set(whichclass(%qe),SC-%qe:rote)][set(#89,LIST_rote:[setunion(%qd,%qe)])]You check in a new rote:&#160;%qe%rYou can now check in its aspects&#160;%(cost and access%).))]

&amp;TRIGGER_STAT/ASPECT-ROTE/LEVEL CharGen Module=@eval [setq(Z,edit(%1,%b,_))][setq(E,edit(%2,%b,_))][setq(Q,statfull(Rote,%qz))][pemit(%0,switch(0,statnum(Rote,%qz),I can't find that Rote.,isrepeat(Rote,%qz),mall(Rote,%qz),match(1 2 3 4 5,%2),The Rote's level should be a whole number between 1 and 5.,[set(#2673,LEVEL-%qq:%2)]You set the level of the&#160;%qq Rote to &quot;%2&quot;.))]

&amp;TRIGGER_STAT/REM-ROTE CharGen Module=@eval [setq(E,edit(%1,%b,_))][setq(F,statfull(rote,%qe))][setq(D,statlist(rote))][pemit(%0,switch(0,statnum(rote,%qe),That rote has not been checked in to the system.,isrepeat(rote,%qe),mall(rote,%qe),[set(whichclass(%qe),SC-%qf:)][set(#89,LIST_rote:[setdiff(%qd,%qf)])][set(#2672,COST-%qf:)][set(#2681,ACCESS-%qf:)]You remove the &quot;%qf&quot; discipline and its associated aspects.))]

