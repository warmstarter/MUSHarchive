&amp;TRIGGER_SET-CONTRACT CharGen Module=@eval [setq(E,edit(%2,%b,_))][setq(T,if(strmatch(%qe,!*),delete(before(%qe,:),0,1),before(%qe,:)))][setq(F,statfull(contract,%qt))][setq(V,after(%qe,:))][setq(D,get(%1/_CONTRACTS))][setq(Q,grab(%qd,%qf:*))][pemit(%0,switch(0,statnum(contract,%qt),I can't find that contract.,isrepeat(CONTRACT,%qt),mall(CONTRACT,%qt),not(strmatch(%2,!*)),if(setr(M,match(%qd,%qf:*)),[set(%1,_CONTRACTS:[ldelete(%qd,%qm)])]You remove the&#160;%qf contract from [name(%1)]'s sheet.,[name(%1)] doesn't have the&#160;%qf contract on [poss(%1)] sheet.),match(lnum(1,setr(L,score(%1,limit))),%qv),Please choose a dot value between 1 and&#160;%ql.,not(setr(Z,match(%qd,%qf:*))),[set(%1,_CONTRACTS:[replace(%qd,%qz,%qf:%qv)])]You set [name(%1)]'s&#160;%qf to&#160;%qv.,[set(%1,_CONTRACTS:[trim(%qd&#160;%qf:%qv)])]You set [name(%1)]'s&#160;%qf to&#160;%qv.))]

&amp;TRIGGER_RAISE-CONTRACT CharGen Module=@eval [setq(E,edit(%2,%b,_))][setq(F,statfull(contract,%qe))][setq(A,get(%1/_contractS))][setq(Q,if(match(%qa,%qf:*),after(grab(%qa,%qf:*),:),0))][pemit(%0,switch(0,setr(M,statnum(contract,%qe)),I can't find that contract.,isrepeat(contract,%qe),mall(contract,%qe),lt(%qq,setr(L,score(%1,limit))),[name(%1)]'s&#160;%qf is already at the limit of&#160;%ql dots.,[set(%1,_contractS:[replace(%qa,%qm,%qf:[setr(I,inc(%qq))])])]You raise [name(%1)]'s&#160;%qf contract to&#160;%qi.))]

&amp;TRIGGER_LOWER-CONTRACT CharGen Module=@eval [setq(E,edit(%2,%b,_))][setq(F,statfull(contract,%qe))][setq(A,get(%1/_contractS))][setq(Q,if(match(%qa,%qf:*),after(grab(%qa,%qf:*),:),0))][pemit(%0,switch(0,setr(M,statnum(contract,%qe)),I can't find that contract.,isrepeat(contract,%qe),mall(contract,%qe),gt(%qq,1),[name(%1)]'s&#160;%qf is already at 1 dot&#160;%(&quot;use sheet/set [name(%1)]/contract=!%qf&quot; to remove the contract from [poss(%1)] sheet%).,[set(%1,_contractS:[replace(%qa,%qm,%qf:[setr(I,dec(%qq))])])]You lower [name(%1)]'s&#160;%qf contract to&#160;%qi.))]

