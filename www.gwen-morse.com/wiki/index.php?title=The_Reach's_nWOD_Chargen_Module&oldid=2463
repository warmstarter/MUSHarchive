&amp;TRIGGER_SET-NUMINA CharGen Module=@eval [setq(E, edit(%2,&#160;%b, _))][setq(T, if(strmatch(%qe,&#160;!*), delete(%qe, 0, 1),&#160;%qe))][setq(F, statfull(numina,&#160;%qt))][setq(D, get(%1/_NUMINA))][pemit(%0, switch(0, statnum(numina,&#160;%qt), I can't find that numina., isrepeat(numina,&#160;%qt), mall(numina,&#160;%qt), not(strmatch(%2,&#160;!*)), if(match(%qd,&#160;%qf), [set(%1, _NUMINA:[setdiff(%qd,&#160;%qf)])]You remove the&#160;%qf Numina from [name(%1)]'s sheet., [name(%1)] doesn't have the&#160;%qf numina on [poss(%1)] sheet.), if(match(%qd,&#160;%qf), [name(%1)] already has the&#160;%qf numina., [set(%1, _NUMINA:[setunion(%qd,&#160;%qf)])]You add the&#160;%qf Numina to [name(%1)]'s sheet.)))]

&amp;TRIGGER_STAT/ADD-NUMINA CharGen Module=@eval [setq(E, edit(%1,&#160;%b, _))][setq(D, statlist(numina))][pemit(%0, switch(0, not(match(%qd,&#160;%qe)), That numina has already been checked in to the system., eq(u(F_LETTERS,&#160;%qe), 0), Numina names may only contain letters%, underscores and hyphens., [set(#89, LIST_NUMINA:[setunion(%qd,&#160;%qe)])]You check in a new numina:&#160;%qe))]

&amp;TRIGGER_STAT/REM-NUMINA CharGen Module=@eval [setq(E, edit(%1,&#160;%b, _))][setq(F, statfull(numina,&#160;%qe))][setq(D, statlist(numina))][pemit(%0, switch(0, statnum(numina,&#160;%qe), That numina has not been checked in to the system., isrepeat(numina,&#160;%qe), mall(numina,&#160;%qe), [set(#89, LIST_NUMINA:[setdiff(%qd,&#160;%qf)])]You remove the &quot;%qf&quot; numina.))]

