&amp;TRIGGER_SET-VICE-POWER CharGen Module=@eval [setq(E,edit(%2,%b,_))][setq(T,if(strmatch(%qe,!*),delete(before(%qe,:),0,1),before(%qe,:)))][setq(F,statfull(Vice-Power,%qt))][setq(V,after(%qe,:))][setq(D,get(%1/_Vice-PowerS))][setq(Q,grab(%qd,%qf:*))][pemit(%0,switch(0,statnum(Vice-Power,%qt),I can't find that Vice-Power.,isrepeat(Vice-Power,%qt),mall(Vice-Power,%qt),not(strmatch(%2,!*)),if(setr(M,match(%qd,%qf:*)),[set(%1,_Vice-PowerS:[ldelete(%qd,%qm)])]You remove the&#160;%qf Vice-Power from [name(%1)]'s sheet.,[name(%1)] doesn't have the&#160;%qf Vice-Power on [poss(%1)] sheet.),match(lnum(1,setr(L,score(%1,limit))),%qv),Please choose a dot value between 1 and&#160;%ql.,not(setr(Z,match(%qd,%qf:*))),[set(%1,_Vice-PowerS:[replace(%qd,%qz,%qf:%qv)])]You set [name(%1)]'s&#160;%qf to&#160;%qv.,[set(%1,_Vice-PowerS:[trim(%qd&#160;%qf:%qv)])]You set [name(%1)]'s&#160;%qf to&#160;%qv.))]

