@@The following are the character sheet commands and functions that need to be in place to make sure that a player can see their sheet, XP, regain willpower, spend pools, etc.@@

@create Character Sheet Functions &lt;CSF>=10
&amp;FN-FORMAT Character Sheet Functions &lt;CSF>=switch(%0,*/s*/p* *=*,if(hasattrp(me,layout-%qV),u(layout-%qV,%1),u(layout-%qR-%qV,%1)),*/p*/s* *=*,if(hasattrp(me,layout-%qV),u(layout-%qV,%1),u(layout-%qR-%qV,%1)),*/p*/s* *,u(layout-sheet,%1),*/s*/p* *,u(layout-sheet,%1),*/s* *,if(hasattrp(me,layout-%qV),u(layout-%qV,%1),u(layout-%qR-%qV,%1)),*/s*,u(layout-sheet,%1),*/n*,u(layout-note,xget(%1,_n-%qV),%1,xget(%1,_nl-%qV)),*/c*,GAME: [name(%1)][switch(%0,*/p*,,*/l*,%bloudly)] +proves[switch(%0,*/p*,%bprivately to [elist(iter(%qP,name(itext(0)), ,|),,|)] that)] [obj(%1)] has a [name(%qS)] with a Resources cost of at least %qV.,*/q*,GAME: [name(%1)][switch(%0,*/p*,,*/l*,%bloudly)] +proves[switch(%0,*/p*,%bprivately to [elist(iter(%qP,name(itext(0)), ,|),,|)] that)] [obj(%1)] has a [name(%qS)] with a quality level of at least %qV. This bonus applies to rolls involving [elist(xget(%qS,Stat),,|)].,*/i*,GAME: [name(%1)][switch(%0,*/p*,,*/l*,%bloudly)] +proves[switch(%0,*/p*,%bprivately to [elist(iter(%qP,name(itext(0)), ,|),,|)] that)] [obj(%1)] has a [name(%qS)] with a quality level of at least %qV. This bonus applies to rolls involving [elist(xget(%qS,Stat),,|)]. This is a resources [xget(%qS,cost)] [xget(%qS,class)],if(isnum(%qV),GAME: [name(%1)][switch(%0,*/p*,,*/l*,%bloudly)] +proves[switch(%0,*/p*,%bprivately to [elist(iter(%qP,name(itext(0)), ,|),,|)] that)] [obj(%1)] [getstatname(%qS)] of at least %qV.,GAME: [name(%1)][switch(%0,*/p*,,*/l*,%bloudly)] +proves[switch(%0,*/p*,%bprivately to [elist(iter(%qP,name(itext(0)), ,|),,|)] that)] [poss(%1)] [getstatname(%qS)] is "[getstat(%1,%qS)]".))
&amp;FN-LISTUNIQUE Character Sheet Functions &lt;CSF>=xget(%0,_race)|[xget(%0,_faction)]|[xget(%0,_group)]|[xget(%0,_tradition)]|[xget(%0,_seeming)]|[xget(%0,_kith)]|[xget(%0,_motley)]|[xget(%0,_entitlement)]|[xget(%0,_court)]|[xget(%0,_path)]|[xget(%0,_order)]|[xget(%0,_cabal)]|[xget(%0,_legacy)]|[xget(%0,_clan)]|[xget(%0,_covenant)]|[xget(%0,_coterie)]|[xget(%0,_bloodline)]|[xget(%0,_auspice)]|[xget(%0,_tribe)]|[xget(%0,_pack)]|[xget(%0,_lodge)]
&amp;FN-PASSLOCK Character Sheet Functions &lt;CSF>=or(isstaff(%0),strmatch(%0,%1),strmatch(xget(%1,_owner),%0),strmatch(owner(%1),%0),strmatch(setr(0,xget(%1,%2)),all),and(!hasattr(%1,%2),!hastype(%1,PLAYER),nearby(%0,%1)),and(eq(ladd(iter(%q0,switch(itext(0),staffonly,0,staff,0,*>=*,and(gte(getstat(%0,first(itext(0),>)),rest(itext(0),=)),hasflag(%0,APPROVED)),*&lt;=*,and(lte(getstat(%0,first(itext(0),&lt;)),rest(itext(0),=)),hasflag(%0,APPROVED)),*=*,and(strmatch(getstat(%0,first(itext(0),=)),rest(itext(0),=)),hasflag(%0,APPROVED)),*>*,and(gt(getstat(%0,first(itext(0),>)),rest(itext(0),>)),hasflag(%0,APPROVED)),*&lt;*,and(lt(getstat(%0,first(itext(0),&lt;)),rest(itext(0),&lt;)),hasflag(%0,APPROVED)),here,nearby(%0,%1),visible,and(nearby(%0,%1),cansee(%1,%0)),invisible,and(nearby(%0,%1),!cansee(%1,%0)),approved,hasflag(%0,APPROVED)),|)),words(%q0,|)),gt(words(%q0,|),0)))
&amp;FN-NUMBERSORT Character Sheet Functions &lt;CSF>=ncomp(strip(%0,0123456789,1),strip(%1,0123456789,1))
&amp;FN-CHECKSTAT Character Sheet Functions &lt;CSF>=if(!isstat(setr(S,first(%0,%1))),"%qS" is not a stat.%b,setq(R,rest(%0,%2))[if(switch(getstattype(%qS),Text,1,0),if(isnum(%qR),%qS requires a text value. You entered "%qR".%b,if(hasattrp(me,limit-[edit(%qS,%b,_)]),if(!t(match(v(limit-%qS),%qR,|)),You must use either [elist(v(limit-[edit(%qS,%b,_)]),or,|)] for a %qS.%b))),if(!isnum(%qR),You must choose a number for the value of %qS.%b,if(or(gt(%qR,5),lt(%qR,0)),The value for %qS must be a number between 0 and 5. You set it at %qR.%b)))])
&amp;FN-NOTEFILTER Character Sheet Functions &lt;CSF>=u(fn-passlock,%1,%2,edit(%0,_n-,_nl-))
&amp;FN-GETNOTELIST Character Sheet Functions &lt;CSF>=sortby(fn-numbersort,filter(fn-notefilter,lattr(%1/_n-*),,,%0,%1))
&amp;FN-GETNOTENUMBER Character Sheet Functions &lt;CSF>=rest(extract(u(fn-getnotelist,%0,%1),%2,1),-)
&amp;FILTER-HASXPSPEND Character Sheet Functions &lt;CSF>=or(t(lattr(%0/_raise-*)), t(lattr(%0/_newspec-*)))
@lock/UseLock Character Sheet Functions &lt;CSF>=STAFFLOCK/1
&amp;OLDFN-GETSTAT Character Sheet Functions &lt;CSF>=if(and(isskill(setr(0,switch(%1,temp*,rest(orest(%1,-)),orest(%1,-)))),!t(setr(1,switch(%1,temp*,gettempstat(%0,rest(%1)),getstat(%0,%1))))),if(t(match(Academics Computer Crafts Investigation Medicine Occult Politics Science,getstatname(%q0))),-3,-1),%q1)
@set Character Sheet Functions &lt;CSF>=INHERIT
@rxlevel Character Sheet Functions &lt;CSF>= Real
@txlevel Character Sheet Functions &lt;CSF>= Real
@parent Character Sheet Functions &lt;CSF>=#2633

@create Character Sheet Commands &lt;CSC>=10
&amp;CMD-+SHEET Character Sheet Commands &lt;CSC>=$+sheet:@pemit %#=[u(layout-sheet,%#)];
&amp;CMD-+STATS Character Sheet Commands &lt;CSC>=$+stats*:@force %#=+sheet%0;
&amp;CMD-+SHEET_&lt;NAME> Character Sheet Commands &lt;CSC>=$+sheet *:@eval setq(E,if(!t(setr(P,pmatch(%0),Player)),Could not find player "%0".)); @switch %qE=,{@pemit %#=[u(layout-sheet,%qP)];},{@pemit %#=[u(prefix)] %qE;}
@lock Character Sheet Commands &lt;CSC>/CMD-+SHEET_&lt;NAME>
&amp;~CMD-+SHEET_&lt;NAME> Character Sheet Commands &lt;CSC>=[u(stafflock,%#)]
&amp;CMD-+PROVE Character Sheet Commands &lt;CSC>=$+prove*:@eval setq(E,switch(%0,*/p*,iter(rest(first(%0,=)),if(!t(pmatch(itext(0))),Could not find player "[itext(0)]".%b,setq(P,%qP [pmatch(itext(0))],Players)), ,@@()),*/s*,,*/n*,,*/l*,,*/i*,,*/q*,,*/c*,,*/*,You entered invalid options for this command. Please check your syntax and try again.%b)[switch(%0,*/p*,,setq(P,iter(lcon(%L),if(cansee(%#,itext(0)),itext(0),setq(N,itext(0),NoSee))),Players))][switch(%0,*/l*,setq(P,setunion(%qP,%qN),Players)[setq(N,,NoSee)])][setq(R,getstat(%#,Race))][setq(S,switch(%0,*=*=*,extract(%0,2,1,=),*/p*=*,rest(%0,=),*=*,orest(first(%0,=)),orest(%0)),Stat)][setq(V,last(orest(%0),=),Value)][switch(%0,*/p*/s* *=*,,*/s*/p* *=*,,*/p*/s* *,setq(V,sheet,Value),*/s*/p* *,setq(V,sheet,Value),*/s* *,,*/s*,setq(V,sheet,Value),*/n*,,*/i*,setq(S,locate(%#,%qS,i))[switch(%0,*/i* *=*,,*/i* *,setq(V,xget(%qS,quality)))],*/c*,setq(S,locate(%#,%qS,i)),*/q*,setq(S,locate(%#,%qS,i)),if(strmatch(%qS,%qV),setq(V,getstat(%#,%qS),Value)))][switch(%0,*/q*=*,,*/q*,setq(V,xget(%qS,quality)),*/c*=*,,*/c*,setq(V,xget(%qS,cost)))][if(and(switch(%0,*/i*,1,*/q*,1,*/c*,1,0),!t(%qS)),You don't have that item.%b)][switch(%0,*/s*,if(and(t(%qV),!hasattrp(me,layout-%qV),!hasattrp(me,layout-%qR-%qV)),"%qV" is not a valid section of your sheet.%b),*/n*,if(!isnum(%qV),%qV is not a number.%b,if(!hasattr(%#,_n-%qV),You do not have a note #%qV.)),*/q*,if(!isnum(%qV),You must enter a number for the quality level of the item you wish to +prove.,if(!lte(%qV,xget(%qS,quality)),That item does not have a quality level of or less than %qV.)),*/c*,if(!isnum(%qV),You must enter a number for the resources cost of the item you wish to +prove.,if(!lte(%qV,xget(%qS,cost)),That item does not have a cost of or less than %qV.)),*/i*,,if(!isstat(%qS),"%qS" is not a stat.%b,if(and(isnum(%qV),!lte(%qV,getstat(%#,%qS))),You do not have a [getstatname(%qS)] stat of at least %qV.%b)))][setq(T,u(fn-format,%0,%#),Text)]); @switch %qE=,{@pemit/list %l %# [setdiff(%qP,%#)] [setdiff(iter(%qP,loc(itext(0))),%l)]=switch(%0,*/s*,stdheader(+prove,OOC)%R %bGAME: %N[switch(%0,*/p*,,*/l*,%bloudly)] +proves part of %p sheet[switch(%0,*/p*,%bprivately to [elist(iter(%qP,name(itext(0)), ,|),,|)])]...%R%qT%R[stdfooter(Verification Code: [default(##/_vcode,not set!)],OOC)],*/n*,stdheader(+prove,OOC)%R %bGAME: %N[switch(%0,*/p*,,*/l*,%bloudly)] +proves a +note[switch(%0,*/p*,%bprivately to [elist(iter(%qP,name(itext(0)), ,|),,|)])]...%R%qT%R[stdfooter(Verification Code: [default(##/_vcode,not set!)],OOC)],%qT (Verification Code: [default(##/_vcode,not set!)])); @pemit/list %qN=GAME: %N +proves something to the room. You must be visible to view this +prove, have it paged directly to you, or ask %o to +prove/l so the whole room will see it.;},{@pemit %#=u(prefix) %qE;}
@set Character Sheet Commands &lt;CSC>/CMD-+PROVE = no_parse
&amp;CMD-+HURT Character Sheet Commands &lt;CSC>=$+hurt*:@eval setq(E,switch(%0,*/b*,setq(T,bashing,Type),*/l*,setq(T,lethal,Type),*/a*,setq(T,agg,Type),*/*,That type of damage was not recognized. Check your syntax and try again.%b,setq(T,bashing,Type))[switch(%0,* *,if(!isnum(orest(%0)),You must enter a number to damage yourself with.%b,setq(D,orest(%0),Damage)),setq(D,1,Damage))][if(gt(%qD,99),You can't do more than 99 points of damage at a time.%b)][if(hasflag(%#,DEAD),You are already dead.)]); @switch %qE=,{@pemit/list %l [lcon(%l)]=GAME: %N causes %oself %qD %qT damage.[setq(H,getstat(%#,Health))] (Verification code: [default(##/_vcode,not set!)]); @dolist lnum(%qD)={@eval null(setq(A,xget(%#,_agg))[setq(L,xget(%#,_lethal))][setq(B,xget(%#,_bashing))][if(!t(%qA),setq(A,0))][if(!t(%qL),setq(L,0))][if(!t(%qB),setq(B,0))][if(gt(add(xget(%#,_[setr(S,%qT)]),1),sub(%qH,switch(%qT,b*,add(%qL,%qA),l*,%qA,0))),setq(S,switch(%qT,b*,lethal,agg)))][if(gt(add(xget(%#,_%qS),1),sub(%qH,switch(%qS,l*,%qA,0))),setq(S,agg))]); &amp;_%qS %#=add(if(t(setr(V,xget(%#,_%qS))),%qV,0),1); &amp;_%qS-times %#=setunion(xget(%#,_%qS-times),add(secs(),#@));}; @wait .1={@eval null(setq(A,xget(%#,_agg))[setq(L,xget(%#,_lethal))][setq(B,xget(%#,_bashing))][if(!t(%qA),setq(A,0))][if(!t(%qL),setq(L,0))][if(!t(%qB),setq(B,0))]); @switch gt(ladd(%qA %qL %qB),%qH)=1,{&amp;_lethal %#=if(gt(%qL,sub(%qH,%qA)),sub(%qH,%qA),%qL); &amp;_bashing %#=if(gt(%qB,sub(%qH,add(%qA,%qL))),sub(%qH,add(%qA,%qL)),%qB); @switch lte(setr(B,xget(%#,_bashing)),0)=1,{&amp;_bashing %#=0; @wipe %#/_bashing-times;},{&amp;_bashing-times %#=extract(xget(%#,_bashing-times),1,%qB);}; @switch lte(setr(L,xget(%#,_lethal)),0)=1,{&amp;_lethal %#=0; @wipe %#/_lethal-times;},{&amp;_lethal-times %#=extract(xget(%#,_lethal-times),1,%qL);}; @switch lte(setr(A,xget(%#,_agg)),0)=1,{&amp;_agg %#=0; @wipe %#/_agg-times;},{&amp;_agg-times %#=extract(xget(%#,_agg-times),1,%qA);};}; @switch gte(if(t(setr(A,xget(%#,_agg))),%qA,0),%qH)=1,{@pemit/list %l [lcon(%l)]=GAME: %N is dead. (Verification code: [default(##/_vcode,not set!)]); @set %#=DEAD;},{@pemit %#=GAME: Your health level: [squish(u(layout-health,%#))] (Verification code: [default(%#/_vcode,not set!)]);};};},{@pemit %#=u(prefix) %qE;}
@set Character Sheet Commands &lt;CSC>/CMD-+HURT = no_parse
&amp;CMD-+HEAL Character Sheet Commands &lt;CSC>=$+heal*:@eval setq(E,switch(%0,*/b*,setq(T,bashing,Type),*/l*,setq(T,lethal,Type),*/a*,setq(T,agg,Type),*/*,That type of damage was not recognized. Check your syntax and try again.%b,setq(T,bashing,Type))[switch(%0,* *,if(!isnum(orest(%0)),You must enter a number to heal yourself by.%b,setq(D,orest(%0),Damage)),setq(D,1,Damage))][if(lt(sub(if(t(setr(V,xget(%#,_%qT))),%qV,0),%qD),0),You don't have that much %qT damage.%b)][if(hasflag(%#,DEAD),You are dead and may not be revived.%b)]); @switch %qE=,{@pemit/list %l [lcon(%l)]=GAME: %N heals %oself of %qD %qT damage. (Verification code: [default(##/_vcode,not set!)])[setq(H,getstat(%0,Health))]; @dolist lnum(%qD)={&amp;_%qT %#=sub(if(t(setr(V,xget(%#,_%qT))),%qV,0),1); &amp;_%qT-times %#=revwords(rest(revwords(sortbynum(xget(%#,_%qT-times)))));}; @wait .1={@pemit %#=GAME: Your health level: [squish(u(layout-health,%#))] (Verification code: [default(%#/_vcode,not set!)]);};},{@pemit %#=u(prefix) %qE;}
