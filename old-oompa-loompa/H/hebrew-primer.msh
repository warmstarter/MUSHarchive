&DO_SHAPE Hebrew Primer=$shape *:@switch 0=[member(v(words),%0)],{@pemit %#=I only know these letters: [v(words)]}, {@emit [get_eval(me/%0)]}
&CHARS Hebrew Primer=a b g d h v z q t y c l m n s i p x k r S T C M N P X
&WORDS Hebrew Primer=aleph bet gimel dalet he vav zayin chet tet yod kaf lamed mem nun samech ayin peh tzadi kuf resh shin taf chaf. mem. nun. feh. tzadi.
&CALC_ONE Hebrew Primer=[mul(add(1,mod(sub(%0,1),9)),power(10,div(%0,9)))]
&ADD_NUMS Hebrew Primer=add(%0,%1)
&CALC_LIST Hebrew Primer=[fold(add_nums,map(calc_one,%0))]
&CALC_CHARS Hebrew Primer=[edit(iter(%0,extract(v(chars),##,1)),%b,)]
&CALC_WORDS Hebrew Primer=[edit(iter(%0,extract(v(words),##,1)),%b,-)]
&POSCHAR Hebrew Primer=[member(v(chars),%0)]
&POSWORD Hebrew Primer=[member(v(words),%0)]
&PARSECHARS Hebrew Primer=[map(poschar,iter(lnum(strlen(%0)),mid(%0,##,1)))]
&PARSEWORDS Hebrew Primer=[edit(map(posword,%0,-),-,%b)]
&DO_CALC Hebrew Primer=$calc *: @switch strmatch(%0,*-*)=1, {@pemit %#=[iter(%0,u(calc_list,u(parsewords,##)))]},{@pemit %#=[iter(%0,u(calc_list,u(parsechars,##)))]}
&DO_CONV Hebrew Primer=$conv *: @switch strmatch(%0,*-*)=1, {@pemit %#=[iter(%0,u(calc_chars,u(parsewords,##)))]},{@pemit %#=[iter(%0,u(calc_words,u(parsechars,##)))]}
