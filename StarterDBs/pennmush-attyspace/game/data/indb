]@SPACE_RESET^1^131072
"$@space/res*:&ACTIVE %!=0;@dol lsearch(all,flag,+)={th ifelse(match(loc(##),v(SPACE)),[sdb(p,get(##/SDB),0,sta,land)][sdb(p,get(##/SDB),0,sta,dock)][sdb(p,get(##/SDB),0,loc)],[sdb(p,get(##/SDB),get(loc(##)/SDB),loc)][sdb(p,get(##/SDB),1,sta,dock)])};@emit Space reset and deactivated."
]SPACE^1^0
"#100"
]OUTPUT^1^0
"8"
]@SPACE_RECONS^1^131072
"$@space/recons*:@dol lsearch(all,flag,+)={th set(##,CONSOLES:%b%b)};@dol iter(lkids(#232),lkids(##))={th [setq(0,zone(loc(##)))][set(##,SHIP:%q0)][set(##,SDB:[get(%q0/SDB)])][ifelse(orflags(%q0,+),set(%q0,CONSOLES:[setunion(##,get(%q0/CONSOLES))]),[emit(## Bad console location.)][tel(##,owner(##))][set(get(##/USER),CONSOLE:)][parent(##,)])]};@emit Resetting consoles."
]@SPACE_ACTIV^1^131072
"$@space/activ*:&active %!=1;@tri %!/loop;@emit Spaceclock activated."
]@SPACE_DEACT^1^131072
"$@space/deact*:&ACTIVE %!=0;@emit Spaceclock deactivated."
]@SPACE_LOAD^1^131072
"$@space/load:@dolist lsearch(all,flag,+)=@swi hasattrp(##,CONSOLES)=1,{@emit ##%t%t%bR:[sdb(read,##)] S:[get(##/SDB)]%t[name(##)]}"
]@SPACE_SAVE^1^131072
"$@space/save:@dolist lsearch(all,flag,+)=@swi hasattrp(##,CONSOLES)=1,{@emit ##%t%t%bW:[sdb(write,##)] S:[get(##/SDB)]%t[name(##)]}"
]@SPACE_DEBUG^1^131072
"$@space/debug:@dolist lsearch(all,flag,+)=@swi hasattrp(##,CONSOLES)=1,{@emit ##%t%tD:[sdb(do,get(##/SDB),0)] S:[get(##/SDB)]%t[name(##)]}"
]LOOP^1^0
"@tri ifthen(v(ACTIVE),set(%!,strcat(OUTPUT:,sdb(i)))%!)/LOOP"
]ACTIVE^2358^0
"0"
]STARTUP^1^65568
"@space/set;@space/load"
]DESCRIBE^1^66080
"The Spaceclock is currently [ifelse(v(ACTIVE),active. [v(OUTPUT)] objects currently iterating.,inactive.)]"
]@SPACE_CLEAR^1^131072
"$@space/clear:&ACTIVE %!=0;@dol lnuma(600)=th strcat(sdb(put,##,0,str,typ),sdb(put,##,0,sta,act),sdb(put,##,0,obj),sdb(put,##,0,loc));@emit Space cleared, and deactivated."
]KEY^1^0
"orflags(%#,W)"
]@SPACE_MAKE^1^131072
"$@space/make * for * in * named *:th setq(0,create(%3,0));@chown %q0=ts;@set %q0=!halt;@set %q0=safe;@set %q0=SO;@set %q0=!halt;@link %q0=#100;@lock %q0=%q0;@lock/enter %q0=%q0;@lock/zone %q0=pmatch(%1);@parent %q0=locate(%#,%0,*);@tel %q0=%2;&masterorg %q0=masterorg(pmatch(%1));&organization %q0=masterorg(pmatch(%1));&location %q0=get(loc(%q0)/SDB);&space %q0=1;&coords %q0=get(loc(%q0)/COORDS);&SDB %q0=words(lsearch(all,flag,+));@pem %#=[name(%q0)]%(%q0%) made and installed with SDB [get(%q0/SDB)]"
]@SPACE_SET^1^131072
